# Local development environment variables
NODE_ENV=development
PORT=3002
ENVIRONMENT=local

# AWS LocalStack configuration
AWS_REGION=eu-west-2
AWS_DEFAULT_REGION=eu-west-2
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_ENDPOINT_URL=http://localhost:4566
AWS_S3_BUCKET=trade-exports-data

# Cache configuration
CACHE_S3_SCHEMA=cache

# MDM Integration
MDM_INTEGRATION_ENABLED=true

# Azure MDM Configuration
AZURE_DEFRA_CLOUD_TENANT_ID=defraclouddev.onmicrosoft.com
AZURE_MDM_CLIENT_ID=
AZURE_MDM_CLIENT_SECRET=your-client-secret-here
AZURE_MDM_SUBSCRIPTION_KEY=your-subscription-key-here
AZURE_MDM_INTERNAL_APIM_SCOPE=api://tst-futuretrade-int.defra.gov.uk/.default
AZURE_MDM_INTERNAL_APIM_ENDPOINT=https://tst-internal-gateway.trade.azure.defra.cloud/mdm
AZURE_MDM_GET_INELIGIBLE_ITEMS_ENDPOINT=/trade/nirms/ineligible-items
AZURE_MDM_GET_ISO_CODES_ENDPOINT=/trade/iso-codes

# Ineligible items configuration
INELIGIBLE_ITEMS_S3_FILE_NAME=ineligible-items
INELIGIBLE_ITEMS_SYNC_CRON_SCHEDULE=0 * * * *

# ISO codes configuration
ISO_CODES_S3_FILE_NAME=iso-codes
ISO_CODES_SYNC_CRON_SCHEDULE=0 * * * *

# Packing list configuration
PACKING_LIST_SCHEMA_VERSION=v0.1
